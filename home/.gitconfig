[user]
	name = Josh Nichols
	email = technicalpickles@github.com
	signingkey = E7BB2E6C

[credential]
	helper = osxkeychain

[core]
	excludesfile = /Users/technicalpickles/.gitignore

[hub]
	protocol = https

[alias]
	dc = diff --cached
	st = status
	co = checkout
	ci = commit
	di = diff

[color]
	ui = true

[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f

[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[commit]
	gpgsign = true
[push]
	default = simple
[github]
	user = technicalpickles
